import java.util.ArrayList;
import java.util.Arrays;

public class MyArrayList {

	private Object elements= new ArrayList<>();
	private int capacity;
	public MyArrayList() {
		super();
	}
	
	public boolean add(int size,Object e){
		if(size == ((ArrayList<Object>) elements).size()){
			ensureArray();
		}
		else
			elements.add(e);
		return false;
	}

	private void ensureArray() {
		elements = Arrays.copyOf(elements, 2*((ArrayList<Object>) elements).size()));
		
	}
}
