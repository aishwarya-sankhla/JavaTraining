package com.training;

import java.sql.SQLException;
import java.util.List;

public interface ContactDao {
	public int addNewContact(ContactDetails person,long phoneNo) throws SQLException;
	public int addExistingContact(int contactid,long phone);
	public List<ContactDetails> displayContactsById(int contactId);
	public int deleteContact(int contactId);
	public List<ContactDetails> displayContactByCategory(String category);
	public List<ContactDetails> displayContacts();
	public int editContact(int contactId);
}
