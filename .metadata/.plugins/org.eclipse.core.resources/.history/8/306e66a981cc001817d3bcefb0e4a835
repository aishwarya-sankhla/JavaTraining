package com.training.ifaces;

import java.util.List;

import javax.jws.WebMethod;
import javax.jws.WebParam;
import javax.jws.WebResult;
import javax.jws.WebService;

@WebService
public interface Availability<SpiceJet> {
	@WebMethod
	@WebResult(name="flightDetails")
	public List<com.training.domain.SpiceJet> checkAvailablity(@WebParam(name="source") String source,@WebParam(name="destination")String destination);
}
